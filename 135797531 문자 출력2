for i in range(0, 9, 1):
    if i < 5:
        for k in range(0, 4 - i, 1):
            print('  ', end='')
        for k in range(0, 2 * i + 1, 1):
            print('\u2602', end='')
    else:
        for k in range(0, i - 4, 1):
            print('  ', end='')
        for k in range(0, 2 * (8 - i) + 1, 1):
            print('\u2602', end='')
    print()
